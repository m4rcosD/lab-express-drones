<h2>Available drones</h2>

{{#each drones}}
    <a href='/drones/{{this._id}}/edit'>
      <h2>Name: {{this.name}}</h2>
    </a>
 <h2>propellers: {{this.propellers}}</h2>
  <h2>maxSpeed: {{this.maxSpeed}}</h2>
  <form action="/drones/{{this._id}}/delete" method="GET">
    <button class="btn btn-info">Delete</button>
  </form>
{{/each}}
